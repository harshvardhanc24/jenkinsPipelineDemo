pipeline {
  agent any
  stages {
    stage('Builds') {
      when {
        changeset "refs/heads/main"
      }
      steps {
        echo 'Building'
      }
    }
    stage('Test') {
      when {
        branch "develop"
        manual true
      }
      steps {
        echo 'Tresting'
      }
    }
  }
}
