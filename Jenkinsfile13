pipeline {
  agent any
  stages {
    stage('Builds') {
      when {
        changeset "refs/heads/main"
      }
      steps {
        echo 'Building'
      }
    }
    stage('Test') {
      when {
        branch "develop"
       }
      steps {
      script {
      input message: 'Do you want to run the test stage?', ok: 'Run'
      echo 'Testing'
      }
    }
  }

